{
  "entrypoint": "index.html",
  "shell": "src/my-app.html",
  "fragments": [
    "src/redux/app-actions.html",
    "src/redux/app-reducers.html",
    "src/redux/app-sagas.html",
    "src/redux/app-store.html",
    "src/redux-router.html",
    "src/my-view1.html",
    "src/my-view2.html",
    "src/my-view3.html",
    "src/my-editor.html",
    "src/my-club.html",
    "src/my-login.html",
    "src/my-view404.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [
    {
      "name": "custom-bundled",
      "bundle": true,
      "js": {"minify": false},
      "css": {"minify": true},
      "html": {"minify": true}
    }
  ]
}
